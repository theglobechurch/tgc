{
  "require": {
    "aelvan/imager": "^2.3",
    "craftcms/cms": "^3.5.10",
    "craftcms/redactor": "2.8.1",
    "jalendport/craft-readtime": "1.6.0",
    "misterbk/mix": "1.5.2",
    "putyourlightson/craft-dashboard-begone": "1.0.1",
    "vaersaagod/dospaces": "1.1.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
